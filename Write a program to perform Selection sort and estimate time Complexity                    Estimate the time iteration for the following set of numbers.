#include <stdio.h>
int main()
{
int a[100], n, i, j, position, swap;
int count=0;
printf("Enter number of elements:");
scanf("%d", &n);
printf("Enter Numbers:");
for (i = 0; i < n; i++)
scanf("%d", &a[i]);
for(i = 0; i < n - 1; i++)
{
position=i;
count++;
for(j = i + 1; j < n; j++)
{
if(a[position] > a[j])
position=j;
count++;
}
if(position != i)
{
swap=a[i];
a[i]=a[position];
a[position]=swap;
count++;
}
count++;
}
printf("\nSorted Array:");
for(i = 0; i < n; i++)
printf(" %d", a[i]);
printf(" time complexity : %d",count);
return 0;
}
