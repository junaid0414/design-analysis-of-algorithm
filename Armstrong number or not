#include<stdio.h>  
int main()    
    {    
    int n,r,sum=0,temp;    
    int count=0;
    printf("enter the number=");    
    scanf("%d",&n);    
    temp=n;    
    while(n>0)    
    {    
    count++;
    r=n%10;   
	count++; 
    sum=sum+(r*r*r);  
	count++;   
    n=n/10; 
	count++;    
    }    
    count++;
    if(temp==sum)  
	printf("armstrong  number "); 
	else 
    printf("not armstrong number");    
    printf("%d",count);
    return 0;  
}
